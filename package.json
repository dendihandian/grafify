    "@ryangjchandler/alpine-clipboard": "^2.1.0",
    "alpinejs": "^3.7.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.3",
    "d3": "^6.2.0",
    "laravel-mix": "^6.0.39",
    "postcss": "^8.4.4",
